<script src="http://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
<script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.11.2/jquery-ui.min.js"></script>

<div class="main">
  <table class="todo">
    <thead>
      <th>To Do</th>
    </thead>
    <tbody>
      <% @items.each do |item| %>
        <tr>
          <td><%= item.text %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <table class="doing">
    <thead>
      <th>Doing</th>
    </thead>
    <tbody>
      <% 4.times do %>
        <tr>
          <td>meow</td>
        </tr>
      <% end %>
    </tbody>
  </table>

    <table class="done">
      <thead>
        <th>Done</th>
      </thead>
      <tbody>
        <% 2.times do %>
          <tr>
            <td>Sample</td>
          </tr>
        <% end %>
      </tbody>
  </table>
</div>

